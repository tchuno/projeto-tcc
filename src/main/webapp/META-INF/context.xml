<?xml version="1.0" encoding="UTF-8"?>
<Context path="/gnfe">

	<Realm className="org.apache.catalina.realm.JDBCRealm"
		driverName="org.postgresql.Driver"
		userTable="usuario" userNameCol="login" userCredCol="senha" 
		userRoleTable="role" roleNameCol="nome" 

		connectionURL="jdbc:postgresql://127.0.0.1:5432/gnfe2"
		connectionName="gnfe" connectionPassword="gnfe"
		/>
		
	<Resource
		name="gnfeJDBC"
		auth="Container"
		type="javax.sql.DataSource"
		driverClassName="org.postgresql.Driver"

		url="jdbc:postgresql://127.0.0.1:5432/gnfe2"
		username="gnfe" password="gnfe"
		
	/>

</Context>